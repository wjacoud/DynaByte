<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.ui.gui.employee.coordinator.ImportTestsUI">
    <children>
        <Label fx:id="titleLbl" layoutX="40.0" layoutY="29.0" text="Import File">
            <font>
                <Font name="Verdana Bold" size="18.0" />
            </font>
        </Label>
        <Button fx:id="simpleBtn" layoutX="499.0" layoutY="14.0" mnemonicParsing="false" onAction="#confirm" prefHeight="30.0" prefWidth="80.0" text="Confirm">
            <font>
                <Font name="Verdana Bold" size="12.0" />
            </font>
        </Button>
        <Button fx:id="simpleBtn" layoutX="399.0" layoutY="14.0" mnemonicParsing="false" onAction="#returnBtn" prefHeight="30.0" prefWidth="80.0" text="Return">
            <font>
                <Font name="Verdana Bold" size="12.0" />
            </font>
        </Button>
        <Label fx:id="outputLbl" layoutX="28.0" layoutY="63.0" text="output label">
            <font>
                <Font name="Verdana" size="12.0" />
            </font>
        </Label>
        <ListView fx:id="listView" layoutX="326.0" layoutY="86.0" prefHeight="249.0" prefWidth="246.0" />
        <Button fx:id="fileBtn" layoutX="40.0" layoutY="353.0" mnemonicParsing="false" onAction="#fileBtn" text="Choose one file" />
      <ListView fx:id="errorList" layoutX="28.0" layoutY="86.0" prefHeight="249.0" prefWidth="246.0" />
        <Label fx:id="pathLbl" layoutX="145.0" layoutY="359.0" text="output label">
            <font>
                <Font name="Verdana" size="12.0" />
            </font>
        </Label>
    </children>
</AnchorPane>
