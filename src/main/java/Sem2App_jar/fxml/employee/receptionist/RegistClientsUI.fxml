<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.ui.gui.employee.receptionist.RegistClientUI">
   <children>
      <Label fx:id="titleLbl" layoutX="40.0" layoutY="30.0" text="Register new Client">
         <font>
            <Font name="Verdana Bold" size="18.0" />
         </font>
      </Label>
      <Label fx:id="simpleLbl" layoutX="40.0" layoutY="229.0" text="Phone number:">
         <font>
            <Font name="Verdana" size="12.0" />
         </font>
      </Label>
      <TextField fx:id="nameTxt" layoutX="210.0" layoutY="184.0" prefHeight="25.0" prefWidth="257.0" />
      <TextField fx:id="phoneTxt" layoutX="210.0" layoutY="224.0" prefHeight="25.0" prefWidth="257.0" />
      <Label fx:id="simpleLbl" layoutX="40.0" layoutY="189.0" text="Name:">
         <font>
            <Font name="Verdana" size="12.0" />
         </font>
      </Label>
      <TextField fx:id="tinTxt" layoutX="210.0" layoutY="144.0" prefHeight="25.0" prefWidth="257.0" />
      <TextField fx:id="nhsTxt" layoutX="210.0" layoutY="104.0" prefHeight="25.0" prefWidth="257.0" />
      <Label fx:id="simpleLbl" layoutX="40.0" layoutY="149.0" text="Tax identification number:">
         <font>
            <Font name="Verdana" size="12.0" />
         </font>
      </Label>
      <Label fx:id="simpleLbl" layoutX="40.0" layoutY="109.0" text="Healthcare number:">
         <font>
            <Font name="Verdana" size="12.0" />
         </font>
      </Label>
      <TextField fx:id="citizenTxt" layoutX="210.0" layoutY="64.0" prefHeight="25.0" prefWidth="257.0" />
      <Label fx:id="simpleLbl1" layoutX="40.0" layoutY="369.0" text="Sex:">
         <font>
            <Font name="Verdana" size="12.0" />
         </font>
      </Label>
      <Label fx:id="simpleLbl" layoutX="40.0" layoutY="69.0" text="Citizen card number:">
         <font>
            <Font name="Verdana" size="12.0" />
         </font>
      </Label>
      <RadioButton fx:id="sexF" layoutX="271.0" layoutY="368.0" mnemonicParsing="false" text="Female">
         <toggleGroup>
            <ToggleGroup fx:id="sex" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="sexM" layoutX="210.0" layoutY="368.0" mnemonicParsing="false" text="Male" toggleGroup="$sex" />
      <Label fx:id="simpleLbl2" layoutX="38.0" layoutY="337.0" text="Birth date:">
         <font>
            <Font name="Verdana" size="12.0" />
         </font>
      </Label>
      <DatePicker fx:id="dateTxt" layoutX="208.0" layoutY="332.0" />
      <Button fx:id="simpleBtn" layoutX="516.0" layoutY="343.0" mnemonicParsing="false" onAction="#confirm" prefHeight="30.0" prefWidth="80.0" text="Confirm">
         <font>
            <Font name="Verdana Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="simpleBtn" layoutX="420.0" layoutY="343.0" mnemonicParsing="false" onAction="#returnBtn" prefHeight="30.0" prefWidth="80.0" text="Return">
         <font>
            <Font name="Verdana Bold" size="12.0" />
         </font>
      </Button>
      <Label fx:id="simpleLbl3" layoutX="38.0" layoutY="266.0" text="Email:">
         <font>
            <Font name="Verdana" size="12.0" />
         </font>
      </Label>
      <TextField fx:id="emailTxt" layoutX="209.0" layoutY="261.0" prefHeight="25.0" prefWidth="257.0" />
      <Label fx:id="simpleLbl31" layoutX="38.0" layoutY="300.0" text="Address:">
         <font>
            <Font name="Verdana" size="12.0" />
         </font>
      </Label>
      <TextField fx:id="addressTxt" layoutX="208.0" layoutY="295.0" prefHeight="25.0" prefWidth="257.0" />
   </children>
</AnchorPane>
