<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.ui.gui.client.UpdateDataUI">
   <children>
      <Label fx:id="titleLabel" layoutX="40.0" layoutY="40.0" text="Update your data">
         <font>
            <Font name="Verdana Bold" size="18.0" />
         </font>
      </Label>
      <Label fx:id="simpleLbl" layoutX="40.0" layoutY="92.0" text="Name:">
         <font>
            <Font name="Verdana" size="12.0" />
         </font>
      </Label>
      <TextField fx:id="nameTxt" layoutX="200.0" layoutY="87.0" prefHeight="25.0" prefWidth="257.0" />
      <TextField fx:id="phoneTxt" layoutX="200.0" layoutY="127.0" prefHeight="25.0" prefWidth="257.0" />
      <Label fx:id="simpleLbl" layoutX="40.0" layoutY="132.0" text="Phone number:">
         <font>
            <Font name="Verdana" size="12.0" />
         </font>
      </Label>
      <RadioButton fx:id="sexM" layoutX="200.0" layoutY="211.0" mnemonicParsing="false" text="Male">
         <toggleGroup>
            <ToggleGroup fx:id="sex" />
         </toggleGroup></RadioButton>
      <RadioButton fx:id="sexF" layoutX="258.0" layoutY="211.0" mnemonicParsing="false" text="Female" toggleGroup="$sex" />
      <Label fx:id="simpleLbl" layoutX="40.0" layoutY="212.0" text="Sex:">
         <font>
            <Font name="Verdana" size="12.0" />
         </font>
      </Label>
      <Button fx:id="simpleBtn" layoutX="410.0" layoutY="333.0" mnemonicParsing="false" onAction="#returnBtn" prefHeight="30.0" prefWidth="80.0" text="Return">
         <font>
            <Font name="Verdana Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="simpleBtn" layoutX="506.0" layoutY="333.0" mnemonicParsing="false" onAction="#confirm" prefHeight="30.0" prefWidth="80.0" text="Confirm">
         <font>
            <Font name="Verdana Bold" size="12.0" />
         </font>
      </Button>
      <Label fx:id="simpleLbl2" layoutX="40.0" layoutY="170.0" text="Address:">
         <font>
            <Font name="Verdana" size="12.0" />
         </font>
      </Label>
      <TextField fx:id="addressTxt" layoutX="200.0" layoutY="165.0" prefHeight="25.0" prefWidth="257.0" />
   </children>
</AnchorPane>
