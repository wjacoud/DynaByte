<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.ui.gui.AuthUI">
   <children>
      <Label fx:id="titleLbl" layoutX="40.0" layoutY="40.0" text="Authentication Menu">
         <font>
            <Font name="Verdana Bold" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="emailTxt" layoutX="155.0" layoutY="95.0" prefHeight="25.0" prefWidth="257.0" />
      <Label fx:id="simpleLbl" layoutX="40.0" layoutY="100.0" text="Enter Email:">
         <font>
            <Font name="Verdana" size="12.0" />
         </font></Label>
      <Label fx:id="simpleLbl" layoutX="40.0" layoutY="140.0" text="Enter Password:">
         <font>
            <Font name="Verdana" size="12.0" />
         </font></Label>
      <PasswordField fx:id="passwordTxt" layoutX="155.0" layoutY="135.0" onAction="#confirm" prefHeight="25.0" prefWidth="257.0" />
      <Button fx:id="simpleBtn" layoutX="145.0" layoutY="200.0" mnemonicParsing="false" onAction="#confirm" prefHeight="30.0" prefWidth="80.0" text="Confirm">
         <font>
            <Font name="Verdana Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="simpleBtn" layoutX="38.0" layoutY="200.0" mnemonicParsing="false" onAction="#returnBtn" prefHeight="30.0" prefWidth="80.0" text="Return">
         <font>
            <Font name="Verdana Bold" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
